[Interface]
PrivateKey = {{ item.private_key }}
Address = {{ item.ip.v4 }}
ListenPort = 51820
SaveConfig = true

{% for peer in item.peers %}
[Peer]
# Peer configuration for {{ peer.name }}
PublicKey = {{ peer.public_key }}
AllowedIPs = {{ peer.allowed_ips | join(',') }}
Endpoint = {{ peer.remote_endpoint }}
{% endfor %}
